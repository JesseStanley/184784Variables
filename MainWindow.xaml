<Window x:Class="_184784Variables.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_184784Variables"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <CheckBox x:Name="chkWantIceCream" Unchecked="chkWantIceCream_Checked"  Checked="chkWantIceCream_Checked" ></CheckBox>
        <Label>Enter a letter</Label>
        <TextBox x:Name="txtInput" MaxLength="1" />
        <Button x:Name="btnShowChar" Click="btnShowChar_Click">Show Value</Button>
        <TextBox x:Name="txtoutput"></TextBox>
    </StackPanel>
</Window>
